{
  "name": "gmpixi",
  "version": "0.0.1",
  "description": "An extended library for PixiJS. With websocket support using socket.io.",
  "main": "src/index.js",
  "scripts": {
    "build": "set NODE_ENV=production&webpack --progress",
    "build-minified": "set NODE_ENV=minified&webpack --progress",
    "build-development": "set NODE_ENV=development&webpack --progress",
    "build-test": "set NODE_ENV=debug&webpack --progress",
    "watch": "set NODE_ENV=debug&webpack --progress --watch",
    "node-test": "set NODE_ENV=node&(start \"\" cmd /c node ./test/node-test/index.js)&webpack --progress --watch",
    "php-test": "set NODE_ENV=php&(start \"\" cmd /c php -S localhost:12205 -t ./test/php-test)&webpack --progress --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/letumfalx/GMPixi.git"
  },
  "author": "LetumFalx",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/letumfalx/GMPixi/issues"
  },
  "homepage": "https://github.com/letumfalx/GMPixi#readme",
  "devDependencies": {
    "express": "^4.16.2",
    "pixi.js": "^4.6.2",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "webpack": "^3.10.0"
  }
}
